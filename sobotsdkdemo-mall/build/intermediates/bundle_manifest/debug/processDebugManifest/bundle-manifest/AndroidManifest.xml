        android:label="@string/sobot_demo_app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light"
        android:usesCleartextTraffic="true" >
        <activity
            android:name="com.sobotmall.demo.activity.SplashActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sobotmall.demo.SobotDemoNewActivity" />
        <activity android:name="com.sobotmall.demo.activity.more.SobotDemoMasterActivity" />
        <activity android:name="com.sobotmall.demo.activity.more.SobotPersonSetActivity" />
