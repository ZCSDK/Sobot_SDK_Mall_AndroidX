        <activity android:name="com.sobotmall.demo.activity.product.SobotDemoCustomActivity" />
        <activity android:name="com.sobotmall.demo.activity.product.SobotDemoCloudCallActivity" />
        <activity android:name="com.sobotmall.demo.activity.product.SobotDemoWorkOrderActivity" />

